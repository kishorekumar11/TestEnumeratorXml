<?xml version="1.0" encoding="UTF-8"?>
<security xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="security.xsd">
    <urls authentication="required" prefix="/dcapi/inventory/computers">
        <url path="/" method="GET" operation-type="read" roles="Inventory_Read">
            <param name="computerName" regex="od_cleartextpattern"/>
            <param name="limit" type="int"/>
            <param name="offset" type="int"/>
        </url>
        <url path="/warranties" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/customFields/import" method="POST" operation-type="write" roles="Inventory_Admin">
            <file antivirus="true" name="computerCustomFieldFile" max-size="65536" allowed-extensions="csv">
                <filename regex="uploadedFileName" max-len="230"/>
            </file>
        </url>
        <url path="/customFields/update" method="POST" operation-type="write" roles="Inventory_Admin">
            <inputstream type="JSONObject" max-len="-1" template="customFieldsUpdateTemplate"/>
        </url>
        <url path="/scan" method="POST" operation-type="write" roles="Inventory_Admin">
            <inputstream type="JSONObject" max-len="-1" template="scanAllTemplate"/>
        </url>
        <url path="/scan/stop" method="POST" operation-type="write" roles="Inventory_Admin"/>
        <url path="/scan/isWanComputer" method="POST" operation-type="write" roles="Inventory_Admin">
            <inputstream type="JSONObject" max-len="-1" template="scanTemplate"/>
        </url>
        <url path="/scan/status" method="POST" operation-type="write" roles="Inventory_Admin">
            <inputstream type="JSONObject" max-len="-1" template="scanTemplate"/>
        </url>
        <url path="/services" method="GET" operation-type="read" roles="Inventory_Read">
            <param name="searchString" regex="od_cleartextpattern"/>
            <param name="offset" type="int"/>
            <param name="limit" type="int"/>
        </url>
        <url path="/groups" method="GET" operation-type="read" roles="Inventory_Read">
            <param name="searchString" regex="od_cleartextpattern"/>
            <param name="offset" type="int"/>
            <param name="limit" type="int"/>
        </url>
        <url path="/certificates" method="GET" operation-type="read" roles="Inventory_Read">
            <param name="searchString" regex="od_cleartextpattern"/>
            <param name="offset" type="int"/>
            <param name="limit" type="int"/>
        </url>
    </urls>
    <urls authentication="required" prefix="/dcapi/inventory/computers/[0-9]+" path-regex="true">
        <url path="/productNumber" method="PUT" operation-type="write" path-regex="true" roles="Inventory_Admin">
            <param name="productNumber" regex="od_cleartextpattern"/>
        </url>
        <url path="/scanWarranty" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/scanComputer" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/scanStatus" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/scanInProgress" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/agentUpgradeInProgress" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/summary" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/generalSummary" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/assetSummary" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/osInformation" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/availableHardware" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/hardwareSummary" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/diskUsageSummary" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/[A-Za-z]+" path-regex="true" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/hardware" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/bitlocker" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/bitLockerRecoveryKey" method="GET" operation-type="read" roles="Inventory_Read">
            <param name="driveName" regex="safestring"/>
        </url>
        <url path="/antivirus" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/macFirewall" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/macFileVault" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/firewall" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/security" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/service" method="GET" operation-type="read" roles="Inventory_Read">
            <param name="serviceID" type="long"/>
        </url>
        <url path="/user" method="GET" operation-type="read" roles="Inventory_Read">
            <param name="userID" type="long"/>
        </url>
        <url path="/share/permissions" method="GET" operation-type="read" roles="Inventory_Read">
            <param name="shareID" type="long"/>
        </url>
        <url path="/group" method="GET" operation-type="read" roles="Inventory_Read">
            <param name="groupID" type="long"/>
        </url>
        <url path="/scanHistory" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/history" method="GET" operation-type="read" roles="Inventory_Read">
            <param name="auditID" type="long"/>
        </url>
        <url path="/fileScanDetails" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/alertMessage" method="DELETE" operation-type="write" roles="Inventory_Admin">
            <param name="alertType" regex="safestring"/>
        </url>
        <url path="/customFields" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/groups" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/usbAudit/count" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/certificate/storeScanStatus" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/modernManagementDetails" method="GET" operation-type="read" roles="Inventory_Read"/>
    </urls>
    <urls authentication="required" prefix="/dcapi/inventory/hardware">
        <url path="/[0-9]+" path-regex="true" method="GET" operation-type="read" roles="Inventory_Read"/>
    </urls>

    <urls authentication="required" prefix="/dcapi/inventory/software">
        <url path="/" method="GET" operation-type="read" roles="Inventory_Read">
            <param name="softwareName" type="String"/>
            <param name="softwareType" regex="[0-3]"/>
            <param name="osType" regex="[Mm]ac|[Ww]indows|[lL]inux|[aA]ll"/>
            <param name="offset" type="int"/>
            <param name="limit" type="int"/>
            <param name="filter" regex="unlicensed|ungrouped|uncategorized"/>
        </url>

        <url path="/listAll" method="GET" operation-type="read" roles="Inventory_Read"/>

        <!--Start of Prohibit SW-->

        <url path="/prohibit" method="POST,DELETE" roles="Inventory_Admin" operation-type="write">
            <inputstream type="JSONObject" template="ProhibitSWTemplate" max-len="-1"/>
        </url>
        <url path="/prohibit/exclusions" method="GET" operation-type="read" roles="Inventory_Read">
            <param name="softwareID" type="long"/>
            <param name="exclusionType" regex="customGroup|computers"/>
            <param name="domainID" type="long"/>
            <param name="searchString" regex="od_cleartextpattern"/>
            <param name="limit" type="int"/>
            <param name="offset" type="int"/>
        </url>
        <url path="/prohibit/exclusions" method="PUT" operation-type="write" roles="Inventory_Read">
            <param name="softwareID" type="long"/>
            <inputstream type="JSONObject" template="saveExclusionsTemplate" max-len="-1"/>
        </url>
        <url path="/prohibit/exclusions/domains" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/prohibit/excludedResources" method="GET" operation-type="read" roles="Inventory_Read">
            <param name="softwareID" type="long"/>
        </url>
        <url path="/prohibit/emailAlerts" method="GET" operation-type="read" roles="Inventory_Read"/>

        <url path="/prohibit/emailAlerts/sendTestMail" method="POST" operation-type="write" roles="Inventory_Admin" >
            <inputstream type="JSONObject" template="ProhibitTestMailTemplate" max-len="-1"/>
        </url>
        <url path="/prohibit/emailAlerts" method="PUT" operation-type="write" roles="Inventory_Admin" >
            <inputstream type="JSONObject" template="updateEmailAlertTemplate" max-len="-1"/>
        </url>
        <url path="/prohibit/autoUninstallPolicy" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/prohibit/systemsWithSoftware" method="PUT" operation-type="write" roles="Inventory_Read"/>

        <url path="/prohibit/autoUninstallPolicy" method="PUT" operation-type="write" roles="Inventory_Admin" >
            <inputstream type="JSONObject" template="updateAutoInstallPolicyTemplate" max-len="-1"/>
        </url>

        <url path="/prohibit/userRequest" method="GET" operation-type="read" roles="Inventory_Read">
            <param name="softwareID" type="long"/>
            <param name="resourceID" type="long"/>
        </url>

        <url path="/prohibit/userRequest" method="PUT" operation-type="write" roles="Inventory_Admin" >
            <param name="softwareID" type="long"/>
            <param name="resourceID" type="long"/>
            <inputstream type="JSONObject" template="userReqTemplate" max-len="-1"/>
        </url>

        <url path="/prohibit/messageBox" method="GET" operation-type="read" roles="Inventory_Read"/>

        <!--End of Prohibit SW-->

        <!--Start of Metering-->

        <url path="/metering/rules" method="GET" operation-type="read" roles="Inventory_Read">
            <param name="searchString" regex="od_cleartextpattern"/>
            <param name="limit" type="int"/>
            <param name="offset" type="int"/>
        </url>

        <url path="/metering/[0-9]{1,20}" path-regex="true" method="GET" operation-type="read" roles="Inventory_Read"/>

        <url path="/metering" method="POST" operation-type="write" roles="Inventory_Admin">
            <inputstream type="JSONObject" template="swMeteringRuleTemplate" max-len="-1"/>
        </url>

        <url path="/metering/[0-9]{1,20}" method="PUT" operation-type="write" path-regex="true" roles="Inventory_Admin">
            <inputstream type="JSONObject" template="swMeteringRuleTemplate" max-len="-1"/>
        </url>

        <url path="/metering/[0-9]{1,20}/disable" path-regex="true" method="POST" operation-type="write" roles="Inventory_Admin" />

        <url path="/metering/[0-9]{1,20}/enable" method="POST" operation-type="write" path-regex="true" roles="Inventory_Admin" />
        <url path="/metering/[0-9]{1,20}" path-regex="true" method="DELETE" operation-type="write" roles="Inventory_Admin"/>


        <url path="/metering/disable" method="POST" operation-type="write" roles="Inventory_Admin">
            <inputstream type="JSONObject" template="ruleListTemplate" max-len="-1"/>
        </url>
        <url path="/metering/enable" method="POST" operation-type="write" roles="Inventory_Admin">
            <inputstream type="JSONObject" template="ruleListTemplate" max-len="-1"/>
        </url>
        <url path="/metering" method="DELETE" operation-type="write" roles="Inventory_Admin" >
            <inputstream type="JSONObject" template="ruleListTemplate" max-len="-1"/>
        </url>

        <!--End of Metering-->

        <url path="/recentInstallation" method="GET" operation-type="read" roles="Inventory_Read">
            <param name="limit" type="int" default="5"/>
        </url>

        <url path="/summary" method="GET" operation-type="read" roles="Inventory_Read"/>

        <url path="/[0-9]{1,20}" path-regex="true" method="GET" operation-type="read" roles="Inventory_Read"/>

        <url path="/[0-9]{1,20}/silentSwitches" method="GET" operation-type="read" roles="Inventory_Read" path-regex="true">
            <param name="softwareInstalledID" type="long"/>
        </url>

        <url path="/[0-9]+/silentSwitches" path-regex="true" method="PUT" operation-type="write" roles="Inventory_Admin">
            <inputstream type="JSONObject" template="silentSwitchTemplate" max-len="-1"/>
        </url>
        <url path="/[0-9]+/uninstall" path-regex="true" method="POST" operation-type="write" roles="Inventory_Admin" >
            <inputstream type="JSONObject" template="uninstallTemplate" max-len="-1"/>
        </url>

        <url path="/licenseType" method="PUT" operation-type="write" roles="Inventory_Admin" >
            <param name="licenseTypeID" type="long"/>
            <inputstream type="JSONObject" template="licenseUpdateTemplate" max-len="-1"/>
        </url>
        <url path="/accessType" method="PUT" operation-type="write" roles="Inventory_Admin" >
            <param name="accessTypeID" type="long"/>
            <inputstream type="JSONObject" template="licenseUpdateTemplate" max-len="-1" />
        </url>


        <url path="/category" method="PUT" operation-type="write" roles="Inventory_Admin" >
            <param name="softwareCategoryID" type="long"/>
            <inputstream type="JSONObject" template="licenseUpdateTemplate" max-len="-1" />
        </url>

        <url path="/softwareCategories" method="GET" operation-type="read" roles="Inventory_Read"/>

        <url path="/[0-9]{1,20}/comment" path-regex="true" method="PUT" operation-type="write" roles="Inventory_Admin" >
            <param name="comment" regex="safestring"/>
        </url>

        <url path="/softwareMessageBoxInfo" method="GET" operation-type="read" roles="Inventory_Read"/>

        <url path="/category/[0-9]{1,20}" path-regex="true" method="GET" operation-type="read" roles="Inventory_Read"/>

        <url path="/category/[0-9]{1,20}" path-regex="true" method="DELETE" operation-type="write" roles="Inventory_Admin" />


        <url path="/category" method="DELETE" operation-type="write" roles="Inventory_Admin" >
            <inputstream type="JSONObject" template="deleteCategoryTemplate" max-len="-1"/>
        </url>

        <url path="/category" method="POST" operation-type="write" roles="Inventory_Admin" >
            <inputstream type="JSONObject" template="addOrUpdateCategoryTemplate" max-len="-1"/>
        </url>

        <url path="/category/[0-9]{1,20}" path-regex="true" method="PUT" operation-type="write" roles="Inventory_Admin" >
            <inputstream type="JSONObject" template="addOrUpdateCategoryTemplate" max-len="-1"/>
        </url>

        <url path="/group/operatingSystem" method="GET" operation-type="read" roles="Inventory_Read"/>

        <url path="/group/[0-9]{1,20}" path-regex="true" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/group" method="POST,PUT" roles="Inventory_Admin" operation-type="write">
            <inputstream type="JSONObject" template="SoftwareGroupTemplate" max-len="-1"/>
        </url>

        <url path="/group/[0-9]{1,20}" path-regex="true" method="DELETE" operation-type="write" roles="Inventory_Admin" />


        <!--Start of License-->

        <url path="/license" method="POST" operation-type="write" roles="Inventory_Admin" >
            <param name="selectedComputerList" regex="longwithcomma" max-len="-1"/>
            <param name="licenseName" regex="safestring" max-len="200" />
            <param name="softwareName" regex="acceptall" max-len="250" />
            <param name="softwareManufacturer" regex="safestring" max-len="250" />
            <param name="softwareVersion" regex="safestring" max-len="255" />
            <param name="licenseOwner" regex="safestring" max-len="200" />
            <param name="noOfCopiesPurchased" regex="longvalue" />
            <param name="licensedTo" regex="safestring" max-len="200" />
            <param name="purchasedDate" regex="^\w{2}\/\w{2}\/\w{4}$" max-len="200" />
            <param name="expiryDate" regex="^\w{2}\/\w{2}\/\w{4}$" max-len="200" />
            <param name="licenseKey" regex="safestring" max-len="200" />
            <param name="remarks" regex="safestring" max-len="2000" />
            <file max-size="102400" name="licenseFile"/>
            <file max-size="102400" name="invoiceFile"/>
        </url>
        <url path="license/import" method="POST" operation-type="write" roles="Inventory_Admin">
            <file antivirus="true" name="csvFile" max-size="65536" allowed-extensions="csv"
                  content-type-name="multipart/form-data">
                <filename regex="uploadedFileName" max-len="230"/>
            </file>
        </url>
        <url path="/license" method="DELETE" operation-type="write" roles="Inventory_Admin" >
            <inputstream type="JSONObject" template="licenseTemplate" max-len="-1"/>
        </url>

        <url path="/license/associate" method="GET" operation-type="read" roles="Inventory_Read">
            <param name="status" regex="safestring"/>
            <param name="softwareID" regex="longvalue"/>
            <param name="searchString" regex="od_cleartextpattern"/>
            <param name="limit" type="int"/>
            <param name="offset" type="int"/>
        </url>

        <url path="/[0-9]{1,20}/license/[0-9]{1,20}" path-regex="true" method="GET" operation-type="read" roles="Inventory_Read"/>

        <url path="/[0-9]{1,20}/license/[0-9]{1,20}/file" path-regex="true" method="DELETE" operation-type="write" roles="Inventory_Admin">
            <param name="fileType" regex="safestring" />
        </url>

        <url path="/[0-9]{1,20}/license/[0-9]{1,20}/file" path-regex="true" method="GET" operation-type="read" roles="Inventory_Read"/>

        <url path="/[0-9]{1,20}/license" path-regex="true" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/[0-9]{1,20}/license/[0-9]{1,20}" path-regex="true" method="PUT" operation-type="write" roles="Inventory_Admin">
            <param name="selectedComputerList" regex="longwithcomma" max-len="-1"/>
            <param name="licenseName" regex="safestring" max-len="200" />
            <param name="softwareName" regex="acceptall" max-len="250" />
            <param name="softwareManufacturer" regex="safestring" max-len="250" />
            <param name="softwareVersion" regex="safestring" max-len="255" />
            <param name="licenseOwner" regex="safestring" max-len="200" />
            <param name="noOfCopiesPurchased" regex="longvalue" />
            <param name="licensedTo" regex="safestring" max-len="200" />
            <param name="purchasedDate" regex="^\w{2}\/\w{2}\/\w{4}$" max-len="200" />
            <param name="expiryDate" regex="^\w{2}\/\w{2}\/\w{4}$" max-len="200" />
            <param name="licenseKey" regex="safestring" max-len="200" />
            <param name="remarks" regex="safestring" max-len="2000" />
            <file max-size="102400" name="licenseFile"/>
            <file max-size="102400" name="invoiceFile"/>
        </url>
        <!--End of License-->
    </urls>

    <!--Start  of FileScan-->

    <urls authentication="required" prefix="/dcapi/inventory/fileScanRules">

        <url path="/[0-9]{1,20}" path-regex="true" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/[0-9]{1,20}" path-regex="true" method="PUT" operation-type="write" roles="Inventory_Admin" >
            <inputstream type="JSONObject" template="fileScanRuleTemplate" max-len="-1"/>
        </url>
        <url path="/" method="POST" operation-type="write" roles="Inventory_Admin" >
            <inputstream type="JSONObject" template="fileScanRuleTemplate" max-len="-1"/>
        </url>
        <url path="/[0-9]{1,20}/disable" path-regex="true" method="POST" operation-type="write" roles="Inventory_Admin" />
        <url path="/[0-9]{1,20}/enable" path-regex="true" method="POST" operation-type="write" roles="Inventory_Admin" />
        <url path="/[0-9]{1,20}" path-regex="true" method="DELETE" operation-type="write" roles="Inventory_Admin" />

        <url path="/disable" method="POST" operation-type="write" roles="Inventory_Admin" >
            <inputstream type="JSONObject" template="ruleListTemplate" max-len="-1"/>
        </url>
        <url path="/enable" method="POST" operation-type="write" roles="Inventory_Admin" >
            <inputstream type="JSONObject" template="ruleListTemplate" max-len="-1"/>
        </url>
        <url path="/" method="DELETE" operation-type="write" roles="Inventory_Admin" >
            <inputstream type="JSONObject" template="ruleListTemplate" max-len="-1"/>
        </url>
        <url path="/nextScheduledTime" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/rules" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/extensions" method="GET" operation-type="read" roles="Inventory_Read"/>

    </urls>
    <!--End of FileScan-->


    <!--Start of Inventory Controller-->

    <urls authentication="required" prefix="/dcapi/inventory">

        <url path="/bitlockers/[0-9]{1,20}" path-regex="true" method="GET" operation-type="read" roles="Inventory_Read"/>

        <url path="/generateInventoryHomeSummary" method="POST" operation-type="write" roles="Inventory_Admin" />
        <url path="/summaryGenerationStatus" method="GET" operation-type="read" roles="Inventory_Read"/>

        <url path="/fileScanConfigured" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/inventoryScan" method="GET" operation-type="read" roles="Inventory_Read"/>

        <url path="/alertMessage" method="DELETE" operation-type="write" roles="Inventory_Admin" >
            <param name="alertType" regex="safestring" max-len="30"/>
        </url>

        <url path="/scanSettings" method="GET" operation-type="read" roles="Inventory_Read"/>

        <url path="/scanSettings" method="PUT" operation-type="write" roles="Inventory_Admin" >
            <inputstream type="JSONObject" template="ScanSettingsTemplate" max-len="-1"/>
        </url>

        <url path="/usbAuditTrackingEnableStatus" method="GET" operation-type="read" roles="Inventory_Read"/>


        <url path="/softwareMeteringSettings" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/softwareMeteringSettings/[a-zA-Z]+" path-regex="true" method="POST" operation-type="write" roles="Inventory_Admin"/>

        <url path="/scheduleScan/nextExecutionTime" method="GET" operation-type="read" roles="Inventory_Read">
            <param name="scanType" regex="safestring"/>
        </url>


        <url path="/scheduleScan" method="GET" operation-type="read" roles="Inventory_Read">
            <param name="scanType" regex="safestring"/>
        </url>
        <url path="/scheduleScan/isConfigured" method="GET" operation-type="read" roles="Inventory_Read">
            <param name="scanType" regex="safestring"/>
        </url>

        <url path="/scheduleScan/enable" method="POST" operation-type="write" roles="Inventory_Admin" >
            <param name="scanType" regex="safestring"/>
        </url>

        <url path="/scheduleScan/disable" method="POST" operation-type="write" roles="Inventory_Admin" >
            <param name="scanType" regex="safestring"/>
        </url>

        <url path="/scheduleScan" method="DELETE" operation-type="write" roles="Inventory_Admin" >
            <param name="scanType" regex="safestring"/>
        </url>

        <url path="/scheduleScan" method="PUT" operation-type="write" roles="Inventory_Admin" >
            <inputstream type="JSONObject" template="scheduleScanTemplate" max-len="-1"/>
            <param name="scanType" regex="safestring"/>
        </url>

        <url path="/scheduleScan" method="POST" operation-type="write" roles="Inventory_Admin" >
            <inputstream type="JSONObject" template="scheduleScanTemplate" max-len="-1"/>
            <param name="scanType" regex="safestring"/>
        </url>
        <url path="/customReports" method="GET" operation-type="read" roles="Inventory_Read"/>

        <url path="/configureAlerts" method="GET" operation-type="read" roles="Inventory_Read"/>

        <url path="/configureAlerts" method="PUT" operation-type="write" roles="Inventory_Admin" >
            <inputstream type="JSONObject" template="configureAlertsTemplate" max-len="-1"/>
        </url>

        <url path="/mailServerStatus" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/helpDeskRequestStatus" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/customGroups" method="GET" operation-type="read" roles="Inventory_Read">
            <param name="customGroupName" regex="od_cleartextpattern"/>
            <param name="limit" type="int"/>
            <param name="offset" type="int"/>
        </url>


    </urls>
    <!--End of Inventory Controller-->

    <!--Start of Inventory Alert History Configuration-->
    <urls prefix="dcapi/inventory/alerts" authentication="required">
        <url path="historyDuration" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="historyDuration" method="PUT" operation-type="write" roles="Inventory_Admin" >
            <param name="duration" regex="^[0-9]{1,4}"/>
        </url>
    </urls>
    <!--End of Inventory Alert History Configuration-->

    <!--Start of Inventory Block EXE -->
    <urls prefix="dcapi/inventory/block" authentication="required">
        <url path="/" method="POST" operation-type="write" roles="Inventory_Admin">
            <inputstream type="JSONObject" template="BlockExeTemplate" max-len="-1"/>
        </url>
        <url path="/[0-9]+" method="PUT" operation-type="write" path-regex="true" roles="Inventory_Admin">
            <inputstream type="JSONObject" template="BlockExeTemplate" max-len="-1"/>
        </url>
        <url path="/[0-9]+" method="DELETE" operation-type="write" path-regex="true" roles="Inventory_Admin"/>
        <url path="/" method="DELETE" operation-type="write" roles="Inventory_Admin">
            <inputstream type="JSONObject" template="BlockExeDeleteTemplate" max-len="-1"/>
        </url>
        <url path="/messageForNewPolicy" method="GET" operation-type="read" roles="Inventory_Read"/>
        <url path="/firstPolicy" method="GET" operation-type="read" roles="Inventory_Read"/>
